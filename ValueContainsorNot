import java.util.*;

public class Containsarray {
	
	public static void main(String[] args) {
		
		int a[] = {10,20,30,40}; 
		
		Scanner s = new Scanner(System.in);
		
		System.out.println("Enter the element");
		
		int check = s.nextInt();
		
		boolean output= false;
		
		for (int i=0; i<a.length; i++)
		{
			if(check == a[i])
			{
				 output = true;
				 break;
			}
			else
			{
				output =  false;
			}
		}
		
		if (output == true)
		{
			System.out.println("The number "+check+ " is contains array");
		}
		else
		{
			System.out.println("The number "+check+" is not contains in array");
		}
	}

}
